#include <iostream>

using namespace std;


int main() {
	int jum, i, arr[100],total=0, max;
	cout << "Masukkan jumlah bilangan : ";
	cin >> jum;
	for (i = 0; i < jum; i++) {
		cout << "Masukkan bilangan ke " << i + 1 << " : ";
		cin >> arr[i];
	}
	max = arr[0];
	for (i = 0; i < jum; i++) {
		if (arr[i] > max) {
			max = arr[i];
		}
	}
	cout << "Nilai maksimum : " << max << endl;
	

}
